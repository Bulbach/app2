<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
   http://www.liquibase.org/xml/ns/dbchangelog
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet author="Alex" id="requestMessageId">
        <createTable tableName="request">
            <column name="id" autoIncrement="true" startWith="1" type="java.lang.Long">
                <constraints primaryKey="true" primaryKeyName="request_id"/>
            </column>
            <column name="personal_number" type="java.lang.Long"/>
            <column name="name" type="varchar(56)"/>
            <column name="surname" type="varchar(100)"/>
            <column name="progress_control" type="varchar(30)"/>
            <column name="correlationid" type="varchar(50)"/>
        </createTable>

    </changeSet>
    <changeSet id="response" author="alex">
        <createTable tableName="response">
            <column name="id" autoIncrement="true"  startWith="6" type="java.lang.Long">
                <constraints primaryKey="true" primaryKeyName="response_id"/>
            </column>
            <column name="personal_number" type="java.lang.Long"/>
            <column name="accrual_amount" type="double"/>
            <column name="payable_amount" type="double"/>
            <column name="ordinance_number" type="java.lang.Long"/>
            <column name="date_of_the_decree" type="date"/>
            <column name="code_article" type="double"/>
            <column name="correlationid" type="java.lang.String"/>
        </createTable>
